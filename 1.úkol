class Zvire:
    def __init__(self, jmeno, druh, vaha):
        self.jmeno = jmeno
        self.druh = druh
        self.vaha = vaha

    def __str__(self):
        return f"Zvire se jmenuje {self.jmeno} je to druh {self.druh} a váží {self.vaha} kg."


